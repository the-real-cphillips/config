[user]
    name = Craig Phillips
    email = cphillips@2u.com
[alias]
    bl = blame
    br= branch
    ci = commit
    co = checkout
    d = diff
    dc = diff --cached
    l = log
    p = pull
    rename = "!git branch -m $1 $2 && git push origin :$1 $2 -u #"
    delete-tag = "!git tag -d $1 && git push :refs/tags/$i"
    sh = show
    st = status
[core]
    pager = less -iR
    editor = /usr/bin/vim
	excludesfile = /Users/cphillips/.gitignore
[color]
    ui = true
[push]
    default = simple
[pull]
    rebase = true
[git-up "bundler"]
    check = true
[git-up "fetch"]
    prune = true
[pager]
	branch = false
[init]
	defaultBranch = main
